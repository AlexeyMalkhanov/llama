Subject: [PATCH] HADOOP-5611: Add some missing includes to c++ code to fix compilation on Debian Lenny


diff --git src/c++/pipes/impl/HadoopPipes.cc src/c++/pipes/impl/HadoopPipes.cc
index a0dc1a1..dabaf55 100644
--- src/c++/pipes/impl/HadoopPipes.cc
+++ src/c++/pipes/impl/HadoopPipes.cc
@@ -28,6 +28,7 @@
 #include <stdint.h>
 #include <stdio.h>
 #include <stdlib.h>
+#include <string.h>
 #include <strings.h>
 #include <sys/socket.h>
 #include <pthread.h>
diff --git src/c++/utils/impl/SerialUtils.cc src/c++/utils/impl/SerialUtils.cc
index a0b4692..03d009b 100644
--- src/c++/utils/impl/SerialUtils.cc
+++ src/c++/utils/impl/SerialUtils.cc
@@ -22,6 +22,7 @@
 #include <rpc/types.h>
 #include <rpc/xdr.h>
 #include <string>
+#include <string.h>
 
 using std::string;
 
diff --git src/c++/utils/impl/StringUtils.cc src/c++/utils/impl/StringUtils.cc
index 6924418..0d2a878 100644
--- src/c++/utils/impl/StringUtils.cc
+++ src/c++/utils/impl/StringUtils.cc
@@ -21,6 +21,8 @@
 #include <errno.h>
 #include <stdint.h>
 #include <stdio.h>
+#include <stdlib.h>
+#include <string.h>
 #include <strings.h>
 #include <sys/time.h>
 
